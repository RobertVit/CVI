     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>CVI_FS_C_C_SUPP_INDUSTRY</TDNAME>
      <TDID>TX</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
