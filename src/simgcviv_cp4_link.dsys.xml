     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In dieser IMG-Aktivität ordnen Sie den &lt;DS:DE.PAVIP&gt;VIP-Kennzeichen&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>des Ansprechpartners im</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:GLOS.3526B80EAFAB52B9E10000009B38F974&gt;Debitorenstammsatz&lt;/&gt; die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:DE.BU_PAVIP&gt;VIP-Kennzeichen&lt;/&gt; des Geschäftspartners zu. Diese</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Zuordnung ist für die Synchronisation des Ansprechpartners in der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Customer-Vendor-Integration relevant.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Sie haben die VIP-Kennzeichen des Debitorenstammsatzes in der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>IMG-Aktivität &lt;DS:SIMG.SIMG_CFMENUOLSDOVR7&gt;VIP-Kennzeichen definieren&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>eingestellt.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Sie haben die VIP-Kennzeichen des Geschäftspartners in der IMG-Aktivität</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE> &lt;DS:SIMG._CABP_GPB_TB916&gt;VIP-Einträge definieren&lt;/&gt;&lt;LS&gt; &lt;/&gt;eingestellt.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Die Zuordnung der VIP-Kennzeichen ist in der IMG-Aktivität</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:SIMG.CVI_MAP_CONTACT&gt;Ansprechpartnerzuordnungen aktivieren&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>eingeschaltet.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
