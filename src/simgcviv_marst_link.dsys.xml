      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Cette activité IMG vous permet d&apos;affecter les &lt;DS:DE.BU_MARST&gt;situations</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>de famille&lt;/&gt; du partenaire aux situations de famille du contact dans la</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>fiche client. Cette affectation est pertinente pour la synchronisation</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>du contact dans l&apos;intégration client/fournisseur.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Vous avez défini la situation de famille de la fiche client.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Effectuez ce paramétrage dans l&apos;activité IMG</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:SIMG.OHIX0024&gt;Déterminer situation de famille&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Vous avez défini la situation de famille du  partenaire.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Effecuez ce paramétrage dans l&apos;activité IMG &lt;DS:SIMG._CABP_TB027&gt;Gérer</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>situations de famille.&lt;/&gt;</TDLINE>
      </TLINE>
