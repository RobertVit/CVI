      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGCVI_MAP_BANKDETAILS</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>F</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00015</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Ce Business Add-In (BAdI) est utilisé dans la composante</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;LS&gt;Synchronisation de données de base (AP-MD-BF-SYN)&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>Il vous permet de modifier et d&apos;étendre les affectations des zones des</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>données bancaires dans l&apos;intégration client/fournisseur. Vous pouvez</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>notamment adapter les affectations entre les coordonnées bancaires du</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>partenaire et les coordonnées bancaires de la fiche client/fournisseur.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dans le système standard, le Business Add-In est activé. Le codage par</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>défaut est automatiquement exécuté.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE BADI_ENH_IMPLEMENTATION OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>Voir également :</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Cette définition de BAdI utilise l&apos;interface IF_EX_CVI_MAP_BANKDETAILS.</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Pour obtenir de plus amples informartions, affichez l&apos;interface dans le</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Class Builder.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Méthodes actives par défaut</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_BP_BANKDETAILS_TO_CUSTOMER&gt;Acti</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>f: affecter coordonnées bancaires du partenaire au client&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_BP_BANKDETAILS_TO_VENDOR&gt;Actif:</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>affecter coordonnées bancaire du partenaire au fournisseur&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_CUSTOMER_BANKDETAILS&gt;Actif:</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>affecter coordonnées bancaire du client au partenaire&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_VENDOR_BANKDETAILS&gt;Actif:</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>affecter coordonnées bancaires du fournisseur au partenaire&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>Méthodes d&apos;exemple :</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_BP_BANK_DATA_TO_CUSTOMER&gt;Exempl</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>e: affecter coordonnées bancaires du partenaire au client&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_BP_BANK_DATA_TO_VENDOR&gt;Exemple:</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>affecter coordonnées bancaires du partenaire au fournisseur&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_CUSTOMER_BANK_DATA&gt;Exemple:</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>affecter coordonnées bancaires du client au partenaire&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>&lt;DS:IO.IF_EX_CVI_MAP_BANKDETAILS     MAP_VENDOR_BANK_DATA&gt;Exemple:</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>affecter coordonnées bancaire du fournisseur au partenaire&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
