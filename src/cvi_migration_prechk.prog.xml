    <item>
     <DOKIL>
      <ID>RE</ID>
      <OBJECT>CVI_MIGRATION_PRECHK</OBJECT>
      <LANGU>E</LANGU>
      <TYP>E</TYP>
      <VERSION>0001</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00070</TXTLINES>
