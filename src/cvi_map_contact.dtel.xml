     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>CVI_MAP_CONTACT</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>F</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00006</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Entraîne l&apos;activation des affectations du contact dans l&apos;intégration</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>client/fournisseur.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Procédez à l&apos;activation de l&apos;affectation si les attributs du contact</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>doivent être traités dans la relation partenaire ou la fiche client, et</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>si vous voulez garantir via les affectations des attributs la</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>synchronisation du contact dans le cadre de l&apos;intégration client.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>Vous pouvez synchroniser les attributs du partenaire suivants :</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Numéros du département</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Fonctions</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Procurations</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Code VIP</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Vous avez complètement paramétré le Customizing des attributs du contact</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>sous &lt;LS&gt;Synchronisation de données de base -&gt; Intégration</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>client/fournisseur -&gt; Options du partenaire -&gt; Options de l&apos;intégration</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>client/fournisseur -&gt; Affectation de zones pour l&apos;intégration client -&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Affectation d&apos;attributs -&gt; Contact&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
